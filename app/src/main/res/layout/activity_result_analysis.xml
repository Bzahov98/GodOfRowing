<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultContentAnalysisActivity">

    <!--<include
        android:id="@+id/res_elapsed_aaa"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        layout="@layout/context_result_tab_container"
        />_-->

    <ScrollView
        android:layout_alignParentTop="true"
        android:id="@+id/res_analysis_scroll_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        >

        <RelativeLayout
            android:id="@+id/aa"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.gms.maps.MapView
                android:id="@+id/res_map_analysis"
                android:layout_alignParentTop="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <include
                android:id="@+id/res_elapsed_time"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                layout="@layout/context_result_tab_container"
                android:layout_below="@+id/res_map_analysis"
                />
            <include
                android:id="@+id/res_meters_total"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                layout="@layout/context_result_tab_container"
                android:layout_below="@+id/res_elapsed_time"
                />
            <include
                android:id="@+id/res_speed_average"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                layout="@layout/context_result_tab_container"
                android:layout_below="@+id/res_meters_total"/>

        </RelativeLayout>
    </ScrollView>
</RelativeLayout>